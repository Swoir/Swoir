fn main(preimage: [u8; 13], hash: pub [u8; 32]) {
    let preimage_hash = dep::std::hash::keccak256(preimage, preimage.len() as u32);
    assert(preimage_hash == hash);
}

#[test]
fn test_main() {
    let preimage: [u8; 13] = [0x48, 0x65, 0x6c, 0x6c, 0x6f, 0x2c, 0x20, 0x77, 0x6f, 0x72, 0x6c, 0x64, 0x21];
    let hash: [u8; 32] = [0xb6, 0xe1, 0x6d, 0x27, 0xac, 0x5a, 0xb4, 0x27, 0xa7, 0xf6, 0x89, 0x00, 0xac, 0x55, 0x59, 0xce, 0x27, 0x2d, 0xc6, 0xc3, 0x7c, 0x82, 0xb3, 0xe0, 0x52, 0x24, 0x6c, 0x82, 0x24, 0x4c, 0x50, 0xe4];
    main(preimage, hash);
}
